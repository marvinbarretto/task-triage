{
  "name": "task-triage",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:vitest": "cd projects/angular-foundation && vitest run",
    "test:vitest:watch": "cd projects/angular-foundation && vitest",
    "test:vitest:ui": "cd projects/angular-foundation && vitest --ui",
    "lib:test": "ng test angular-foundation",
    "lib:test:vitest": "npm run test:vitest",
    "lib:build": "ng build angular-foundation",
    "lib:release": "npm run lib:build && cd projects/angular-foundation && standard-version",
    "lib:publish": "cd dist/angular-foundation && npm publish"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.1.2",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/fire": "20.0.1",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@fourfold/angular-foundation": "0.1.6",
    "@fullcalendar/angular": "6.1.18",
    "@fullcalendar/core": "6.1.18",
    "@fullcalendar/daygrid": "6.1.18",
    "@fullcalendar/interaction": "6.1.18",
    "@fullcalendar/timegrid": "6.1.18",
    "@google/generative-ai": "0.24.1",
    "@types/uuid": "10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "@vitest/ui": "3.2.4",
    "jasmine-core": "~5.8.0",
    "jsdom": "26.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "standard-version": "9.5.0",
    "typescript": "~5.8.2",
    "vitest": "3.2.4"
  }
}
